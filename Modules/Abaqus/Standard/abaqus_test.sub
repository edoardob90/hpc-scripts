#!/bin/bash 
#SBATCH --job-name Abaqus
#SBATCH --nodes 1
#SBATCH --ntasks 1
#SBATCH --cpus-per-task 8
#SBATCH --time 00:10:00
 
module purge
module load intel/14.0.1 intelmpi/4.1.3 abaqus/6.14-1/intel-14.0.1

# Abaqus/standard uses thread parallelism
abaqus inter cpus=$SLURM_CPUS_PER_TASK j=umatmst3 user=umatmst3.f